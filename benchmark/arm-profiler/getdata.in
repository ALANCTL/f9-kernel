define print_result
	set $i = 0
	set $MAX = 14
	set $offset = 4
	set $ticks = msTicks
	while $i < $MAX
		printf "%d %f\n", $offset, (result[$i] * msTicks) / 1000.0
		
		set $i = $i + 1
		set $offset = 4 << $i
	end
end 

